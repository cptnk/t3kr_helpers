    "type": "typo3",
    "require": {
        "typo3/cms": "^8.7",
        "php": ">=7.1"
    },
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Stanislaw Gutsch",
            "email": "sg@kreativrudel.de"
        }
    ],
    "keywords": [
        "TYPO3",
        "typo3",
        "helper"
    ],
    "autoload": {
        "psr-4": {
            "Kreativrudel\\Helpers\\": "Classes/"
        }
    },
    "extra": {
